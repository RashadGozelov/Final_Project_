@model Parallax
@{
    ViewData["Title"] = "Detail";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <h3>@Html.Raw(Model.Title1)</h3>
            <p>@Html.Raw(Model.Title2)</p>
            <img src="~/img/@Model.Image" width="220px" />
        </div>
    </div>
    <div class="row mt-4">
        <div class="col-12">
            <a asp-action="Index"><i class="fas fa-arrow-left"></i> Go Back</a>
        </div>
    </div>
</div>


